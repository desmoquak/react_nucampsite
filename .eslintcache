   }\r\n    if (props.errMess) {\r\n        return (\r\n            <div className=\"container\">\r\n                <div className=\"row\">\r\n                    <div className=\"col\">\r\n                        <h4>{props.errMess}</h4>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        );\r\n    }\r\n    if (props.campsite) {\r\n        return (\r\n            <div className=\"container\">\r\n                <div className=\"row\">\r\n                    <div className=\"col\">\r\n                        <Breadcrumb>\r\n                            <BreadcrumbItem><Link to=\"/directory\">Directory</Link></BreadcrumbItem>\r\n                            <BreadcrumbItem active>{props.campsite.name}</BreadcrumbItem>\r\n                        </Breadcrumb>\r\n                        <h2>{props.campsite.name}</h2>\r\n                        <hr />\r\n                    </div>\r\n                </div>\r\n                <div className=\"row\">\r\n                    <RenderCampsite campsite={props.campsite} />\r\n                    <RenderComments\r\n                        comments={props.comments}\r\n                        postComment={props.postComment}\r\n                        campsiteId={props.campsite.id}\r\n                    />\r\n                </div>\r\n            </div>\r\n        );\r\n    }\r\n    return <div />;\r\n}\r\n\r\nexport default CampsiteInfo;","/Users/johnschaub/Desktop/NucampFolder/3-React/nucampsite/src/redux/campsites.js",[],"/Users/johnschaub/Desktop/NucampFolder/3-React/nucampsite/src/redux/comments.js",[],"/Users/johnschaub/Desktop/NucampFolder/3-React/nucampsite/src/redux/partners.js",[],"/Users/johnschaub/Desktop/NucampFolder/3-React/nucampsite/src/redux/forms.js",[],"/Users/johnschaub/Desktop/NucampFolder/3-React/nucampsite/src/redux/promotions.js",[],"/Users/johnschaub/Desktop/NucampFolder/3-React/nucampsite/src/redux/ActionTypes.js",[],"/Users/johnschaub/Desktop/NucampFolder/3-React/nucam